<h1>New offering</h1>

<% form_for(@offering) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :ticket_seller_id %><br />
    <%= f.text_field :ticket_seller_id %>
  </p>
  <p>
    <%= f.label :gig_id %><br />
    <%= f.text_field :gig_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', offerings_path %>
