<% form_for([:admin, @artist]) do |f| %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :album %><br />
    <%= f.text_field :album %>
  </p>
  <p>
    <%= f.label :track %><br />
    <%= f.text_field :track %>
  </p>
  <p>
    <%= f.label :country %><br />
    <%= f.text_field :country %>
  </p>
  <p>
    <%= f.label :web %><br />
    <%= f.text_field :web %>
  </p>
  <p>
    <%= f.label :youtube %><br />
    <%= f.text_field :youtube %>
  </p>
  <p>
    <%= f.label :cover_image %><br />
    <%= f.text_field :cover_image %>
  </p>
  <p>
    <%= f.label :mp3 %><br />
    <%= f.text_field :mp3 %>
  </p>
  <p>
    <%= f.label :tor %><br />
    <%= f.text_field :tor %>
  </p>
  <p>
    <%= f.label :discog_title %><br />
    <%= f.text_field :discog_title %>
  </p>
  <p>
    <%= f.label :discog_detail %><br />
    <%= f.text_area :discog_detail %>
  </p>
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>